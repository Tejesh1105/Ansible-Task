all:
  hosts:
    host1:
      ansible_hosts: Your-Hosts-PrivateIp1
    host2:
      ansible_hosts: Your-Hosts-PrivateIp2
    host3:
      ansible_hosts: Your-Hosts-PrivateIp3   
  children:
    webservers:
      hosts:
        host1:
        host2:
    dbservers:
      hosts:
        host3:
    all_groups:
      children:
        webservers:
        dbservers:        
      vars: 
       ansible_user: ec2-user
       ansible_ssh_private_key_file: privatekey.pem 

  
